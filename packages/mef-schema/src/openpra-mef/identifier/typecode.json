{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "$id": "https://openpra.org/schema/v1/openpra-mef/identifier/typecode.json",
  "title": "Type Code Schema",
  "description": "A type code consisting of three uppercase letters followed by a hyphen and three digits",
  "type": "string",
  "pattern": "^[A-Z]{3}-\\d{3}$",
  "examples": [
    "ABC-123",
    "XYZ-456"
  ],
  "additionalProperties": false
}
