{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "$id": "https://openpra.org/schema/v0/openpra-mef/identifier/semantic-version.json",
  "title": "Semantic Version Schema",
  "description": "Schema for semantic versioning",
  "type": "string",
  "pattern": "^(0|[1-9]\\d*)\\.(0|[1-9]\\d*)\\.(0|[1-9]\\d*)(-(0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*)(\\.(0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*))*)?(\\+[0-9a-zA-Z-]+(\\.[0-9a-zA-Z-]+)*)?$",
  "additionalProperties": false,
  "examples": [
    "1.0.0",
    "2.0.0-alpha",
    "3.0.0-beta.1",
    "4.0.0-beta.2+20130313144700",
    "5.0.0+20130313144700"
  ]
}
