{
  "extends": [
    "plugin:playwright/recommended",
    "plugin:cypress/recommended",
    "plugin:@nx/react",
    "../../../.eslintrc.json"
  ],
  "parserOptions": {
    "project": [
      "packages/frontend/web-editor/tsconfig.json",
      "packages/frontend/web-editor/tsconfig.*?.json"
    ]
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.cy.{ts,js,tsx,jsx}", "cypress/**/*.{ts,js,tsx,jsx}"],
      "rules": {}
    },
    {
      "files": ["e2e/**/*.{ts,js,tsx,jsx}"],
      "rules": {}
    }
  ]
}
